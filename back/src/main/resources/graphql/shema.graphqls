type Query {
  GetUser(id: ID): UserInfo
  GetTeam(id: ID): TeamInfo
  GetAllUsers: [UserInfo]!
  GetAllTeams: [TeamInfo]!
}

type Mutation {
  CreateUser(user: UserInput): UserInfo!
  CreateTeam(team: TeamInput): TeamInfo!
}

input UserInput {
  username: String,
  teamId: ID,
}

input TeamInput {
  name: String,
}

type TeamInfo {
  id: ID,
  name: String,
  createdAt: String
}

type UserInfo{
  id: ID,
  username: String,
  team: TeamInfo,
  createdAt: String
}
