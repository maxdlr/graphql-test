type Query {
  GetUser(id: ID): User
  GetTeam(id: ID): Team
  GetAllUsers: [User]!
  GetAllTeams: [Team]!
}

type Mutation {
  CreateUser(user: UserInput): User!
  CreateTeam(team: TeamInput): Team!
}

input UserInput {
  username: String,
  team: TeamInput,
}

input TeamInput {
  name: String,
}

type Team {
  id: ID,
  name: String,
  createdAt: String
}

type User {
  id: ID,
  username: String,
  team: Team,
  createdAt: String
}
